<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akikec.raspinf" >
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudentActivity"
            android:label="@string/title_activity_student"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.akikec.raspinf.MainActivity" />
        </activity>
        <activity
            android:name=".PrepodActivity"
            android:label="@string/title_activity_prepod"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.akikec.raspinf.MainActivity" />
        </activity>
        <activity
            android:name=".PastRaspActivity"
            android:label="@string/title_activity_past_rasp"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.akikec.raspinf.MainActivity" />
        </activity>
        <activity
            android:name=".PresentRaspActivity"
            android:label="@string/title_activity_present_rasp">
            <!--android:parentActivityName=".MainActivity" >-->
            <!--<meta-data-->
                <!--android:name="android.support.PARENT_ACTIVITY"-->
                <!--android:value="com.example.akikec.raspinf.MainActivity" />-->
        </activity>
        <activity
            android:name=".SettingsRaspActivity"
            android:label="@string/title_activity_settings_rasp" >
        </activity>
        <activity android:name=".Prefs"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".PrefsActivity"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>
